				"searchMicrosoftSymbolServer": true,
				"searchNuGetOrgSymbolServer": true,
				// 符号缓存目录,默认%TEMP%\\SymbolCache,Linux则是~/.dotnet/symbolcache
				// "cachePath": ""
			},
			// sourcelink配置
			"sourceLinkOptions": {
				"*": {
					"enabled": true
